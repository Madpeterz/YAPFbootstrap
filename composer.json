{
    "name": "madpeterz/yapfbootstrap",
    "description": "YAPF - Bootstrap 5 workspace",
    "type": "library",
    "license": "GPL-3.0-only",
    "autoload": {
        "psr-4": {
            "YAPF\\Bootstrap\\": "src/Bootstrap"
        }
    },
    "config": {
        "platform": {
            "php": "8.2"
        }
    },
    "require": {
        "php": ">=8.2",
        "madpeterz/yetonemorephpframework": "8.*",
        "madpeterz/yapfinputfilter": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "authors": [
        {
            "name": "Madpeter",
            "email": "madpeter@gmail.com"
        }
    ],
    "scripts": {
        "phpcs": "phpcs",
        "phpcbf": "phpcbf",
        "phpunit": "phpunit",
        "dev-test": [
            "phpunit --colors=always --configuration phpunit.xml --testdox --testsuite All"
        ]
    }
}